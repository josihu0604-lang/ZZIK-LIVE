{
  "name": "zzik-live",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.10",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint . --max-warnings=0",
    "lint:privacy": "eslint .",
    "lint:why": "npm ls eslint || true",
    "format": "prettier -w .",
    "format:check": "prettier --check .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:security": "vitest run tests/unit/logger.spec.ts tests/unit/rate-limit.spec.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "playwright test tests/e2e/smoke.spec.ts",
    "test:e2e:a11y": "playwright test e2e/a11y.spec.ts",
    "test:components": "vitest run src/components/**/*.spec.tsx",
    "test:a11y": "vitest run tests/accessibility.test.ts",
    "test:aria": "node scripts/validate-aria.mjs",
    "analyze": "source-map-explorer 'build/static/js/*.js' --no-border-checks",
    "type-check": "tsc --noEmit",
    "perf:monitor": "node scripts/performance-report.mjs",
    "k6:smoke": "k6 run k6/api-smoke.js",
    "k6:search": "k6 run k6/search-performance.js",
    "k6:comprehensive": "k6 run k6/api-comprehensive.js",
    "k6:load": "k6 run k6/api-load.js",
    "k6:security": "k6 run k6/security-smoke.js",
    "db:setup": "bash scripts/db-setup.sh",
    "db:up": "docker compose -f docker-compose.yml up -d",
    "db:down": "docker compose -f docker-compose.yml down",
    "db:logs": "docker compose -f docker-compose.yml logs -f db",
    "db:migrate": "prisma migrate deploy",
    "db:indexes": "psql $DATABASE_URL -f prisma/migrations/add_geo_indexes.sql",
    "db:studio": "prisma studio",
    "db:seed": "psql $DATABASE_URL -f scripts/seed-test-data.sql",
    "seed:mini": "psql \"$DATABASE_URL\" -f scripts/seed-mini.sql",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:reset": "npm run db:down && npm run db:up && sleep 5 && npm run db:setup",
    "doctor": "tsx scripts/doctor.ts",
    "clean": "bash scripts/cleanup.sh",
    "headers:verify": "bash scripts/verify-headers.sh",
    "privacy:scan": "bash scripts/privacy-scan.sh",
    "bench:smoke": "bash scripts/bench-smoke.sh",
    "a11y:contrast": "node scripts/contrast-check.mjs",
    "guard:dynamic": "node scripts/guard-dynamic.mjs",
    "security:check": "npm audit && npm run lint:privacy && npm run headers:verify && npm run privacy:scan",
    "verify:pr": "npm run typecheck && npm run lint && npm run format:check && npm run test:coverage && npm run privacy:scan && npm run headers:verify",
    "verify:ui": "npm run typecheck && npm run lint && npm run build && npm run a11y:contrast && npm run test:e2e",
    "verify:uxui": "npm run typecheck && npm run lint && npm run format:check && npm run test:e2e:a11y",
    "verify:full": "npm run verify:pr && npm run k6:comprehensive && npm run test:e2e:a11y",
    "a11y:report": "playwright test --grep @a11y --reporter=list",
    "perf:smoke": "npm run k6:smoke",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "*.{json,md,css}": [
      "prettier -w"
    ]
  },
  "dependencies": {
    "@mapbox/geo-viewport": "^0.4.1",
    "@prisma/client": "^5.18.0",
    "@turf/turf": "^7.0.0",
    "@upstash/redis": "1.35.6",
    "framer-motion": "12.23.24",
    "ioredis": "^5.4.1",
    "jsqr": "1.4.0",
    "lucide-react": "0.553.0",
    "mapbox-gl": "3.16.0",
    "nanoid": "^5.0.7",
    "next": "^15.1.6",
    "ngeohash": "0.6.3",
    "pg": "^8.11.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "server-only": "^0.0.1",
    "supercluster": "^8.0.1",
    "swiper": "12.0.3",
    "uuid": "^9.0.1",
    "web-vitals": "5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@playwright/test": "^1.49.0",
    "@tailwindcss/postcss": "4.1.17",
    "@types/css-modules": "1.0.5",
    "@types/ngeohash": "0.6.8",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/supercluster": "7.1.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "8.46.4",
    "@typescript-eslint/parser": "8.46.4",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "10.4.22",
    "eslint": "^9.5.0",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-security": "^3.0.0",
    "globby": "15.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "playwright": "^1.46.1",
    "postcss": "8.5.6",
    "prettier": "^3.2.5",
    "prisma": "^5.17.0",
    "tailwindcss": "4.1.17",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
