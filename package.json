{
  "name": "zzik-live",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.10",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:api": "bash scripts/test-api-endpoints.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "prepare": "husky",
    "analyze": "ANALYZE=true npm run build",
    "analyze:size": "size-limit",
    "analyze:deps": "depcheck",
    "analyze:circular": "madge --circular --extensions ts,tsx app/ components/ lib/",
    "analyze:dead": "ts-prune",
    "quality": "bash scripts/quality-check.sh",
    "ux": "bash scripts/ux-audit.sh",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "validate:env": "ts-node scripts/validate-env.ts",
    "security": "bash scripts/security-audit.sh",
    "security:fix": "npm audit fix",
    "bundle": "bash scripts/bundle-analysis.sh",
    "icons": "bash scripts/generate-pwa-icons.sh",
    "accessibility": "playwright test tests/e2e/accessibility.spec.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@sentry/nextjs": "10.25.0",
    "ioredis": "^5.8.2",
    "jsqr": "1.4.0",
    "lucide-react": "^0.553.0",
    "mapbox-gl": "3.16.0",
    "nanoid": "5.1.6",
    "next": "16.0.3",
    "ngeohash": "^0.6.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "server-only": "0.0.1",
    "supercluster": "8.0.1",
    "uuid": "13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@next/bundle-analyzer": "16.0.3",
    "@playwright/test": "1.56.1",
    "@tailwindcss/postcss": "4.1.17",
    "@types/ngeohash": "0.6.8",
    "@types/node": "20.19.25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "10.0.0",
    "@vitest/ui": "4.0.9",
    "depcheck": "1.4.7",
    "eslint": "^9",
    "eslint-config-next": "16.0.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-security": "3.0.1",
    "globby": "15.0.0",
    "husky": "9.1.7",
    "lighthouse": "12.8.2",
    "lint-staged": "16.2.6",
    "madge": "8.0.0",
    "node-fetch": "3.3.2",
    "playwright": "1.56.1",
    "prettier": "3.6.2",
    "prisma": "^6.1.0",
    "size-limit": "11.2.0",
    "tailwindcss": "^4",
    "ts-node": "10.9.2",
    "ts-prune": "0.10.3",
    "typescript": "^5",
    "vitest": "4.0.9"
  }
}
